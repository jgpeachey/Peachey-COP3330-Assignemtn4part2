<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<SplitPane prefHeight="169.0" prefWidth="602.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.MainWindowController">
   <items>
      <AnchorPane prefHeight="63.0" prefWidth="600.0">
         <children>
            <ChoiceBox fx:id="choiceBox" layoutX="490.0" layoutY="145.0" prefHeight="25.0" prefWidth="156.0" />
            <TableView fx:id="tableView" layoutY="184.0" prefHeight="351.0" prefWidth="685.0">
              <columns>
                <TableColumn fx:id="itemColumn" prefWidth="103.0" text="Name" />
                <TableColumn fx:id="descriptionColumn" prefWidth="339.0" text="Description" />
                  <TableColumn fx:id="dateColumn" prefWidth="114.0" text="Date" />
                  <TableColumn fx:id="markColumn" prefWidth="128.0" text="Completed" />
              </columns>
            </TableView>
            <Button fx:id="sortButton" layoutX="362.0" layoutY="146.0" mnemonicParsing="false" onAction="#clickedSort" prefWidth="110.0" text="Sort" />
            <Button fx:id="clearListButton" layoutX="39.0" layoutY="144.0" mnemonicParsing="false" onAction="#clickedClearList" prefWidth="110.0" text="Clear List" />
            <Button fx:id="removeItemButton" layoutX="37.0" layoutY="69.0" maxWidth="-Infinity" mnemonicParsing="false" onAction="#clickedRemoveItem" prefWidth="110.0" text="Remove Item" />
            <Button fx:id="markCompleteButton" layoutX="207.0" layoutY="70.0" mnemonicParsing="false" onAction="#clickedMarkComplete" prefWidth="110.0" text="Mark Complete" />
            <Button fx:id="markIncompleteButton" layoutX="208.0" layoutY="144.0" mnemonicParsing="false" onAction="#clickedMarkIncomplete" prefWidth="110.0" text="Mark Incomplete" />
            <Button fx:id="helpButton" layoutX="631.0" layoutY="7.0" mnemonicParsing="false" onAction="#clickedHelp" prefWidth="45.0" text="Help" />
            <Button fx:id="newItemButton" layoutX="357.0" layoutY="69.0" mnemonicParsing="false" onAction="#clickedNewItemButton" prefWidth="110.0" text="Add Item" />
            <Button fx:id="editItemButton" layoutX="511.0" layoutY="68.0" mnemonicParsing="false" onAction="#clickedEditItem" prefWidth="110.0" text="Edit Item" />
            <Button fx:id="saveListButton" layoutX="7.0" layoutY="6.0" mnemonicParsing="false" onAction="#clickedSaveList" prefWidth="45.0" text="Save" />
            <Button fx:id="loadListButton" layoutX="71.0" layoutY="7.0" mnemonicParsing="false" onAction="#clickedLoadList" prefWidth="45.0" text="Load" />
         </children>
      </AnchorPane>
   </items>
</SplitPane>
